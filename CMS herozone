        <script defer="defer" src="https://static1.squarespace.com/static/vta/5c5a519771c10ba3470d8101/scripts/site-bundle.9b819965ab181a3a4e4a2f27d93770b9.js" type="text/javascript"></script>
        <script>
            document.addEventListener("DOMContentLoaded", function() {
                const mcookieContainer = document.querySelector(".mcookie-container");
                const mcookieParallax = document.createElement("div");
                mcookieParallax.className = "mcookie-parallax";
                mcookieContainer.appendChild(mcookieParallax);

                function animate() {
                    mcookieContainer.style.transform = "scale(1.05)";
                    mcookieContainer.style.transition = "transform 0.5s ease-in-out";
                }

                function parallaxScroll() {
                    const scrollPosition = window.pageYOffset;
                    mcookieParallax.style.transform = `translateY(${scrollPosition * 0.5}px)`;
                }

                function fadeInElements() {
                    const fadeElements = document.querySelectorAll('.mcookie-container h2, .mcookie-container p');
                    fadeElements.forEach((element,index)=>{
                        setTimeout(()=>{
                            element.style.opacity = '1';
                            element.style.transform = 'translateY(0)';
                        }
                        , index * 500);
                    }
                    );
                }

                setTimeout(animate, 1000);
                window.addEventListener("scroll", parallaxScroll);
                fadeInElements();
            });
        </script>
        <svg xmlns="http://www.w3.org/2000/svg" version="1.1" style="display:none" data-usage="social-icons-svg">
